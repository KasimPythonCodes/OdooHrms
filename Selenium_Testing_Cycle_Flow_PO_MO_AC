++++++++++++++++++++++Net salary variable +++++++++++++++++++++++++++++++++

days = worked_days.get('WORK100')
worked_days_count = days.number_of_days if days else 0.0
leave = worked_days.get('LEAVE90')
leave_days = leave.number_of_days if leave else 0.0
total_per_days_am = categories.get('BASIC', 0.0)

result = (
    total_per_days_am * worked_days_count
    + categories.get('hra_allowance', 0.0)
    + categories.get('sb_allowance', 0.0)
 
) - (categories.get('esic_empr_deduction', 0.0) + categories.get('pfd_deduction', 0.0))


+++++++++++++++++++++ Basic Salary Variable +++++++++++++++++++++++++++++++

days = worked_days.get('WORK100')
worked_days_count = days.number_of_days if days else 0.0
leave = worked_days.get('LEAVE90')
leave_days = leave.number_of_days if leave else 0.0
month_days = payslip.month_days or 30
result = contract.wage / month_days

                    


